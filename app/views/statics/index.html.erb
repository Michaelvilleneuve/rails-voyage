<section class="home">
    <div class="home-login">
        <h1>Voyages</h1>
        <p>Bienvenue sur voyage</p>
        <%= form_for(:user, :url => session_path(:user)) do |f| %>
            <%= f.text_field :email %>
            <%= f.password_field :password %>
            <%= f.check_box :remember_me %>
            <%= f.label :remember_me %>
            <%= f.submit 'Sign in' %>
            <%= link_to "Forgot your password?", new_password_path(:user) %>
        <% end %>
    </div>
</section>